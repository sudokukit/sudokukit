{
  "name": "@sudokukit/core",
  "version": "0.0.0",
  "description": "Generating & Solving Sudoku's",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "hook:pre-commit": "npm run lint:staged",
    "hook:pre-push": "npm test",
    "lint:staged": "lint-staged --allow-empty",
    "lint:all": "biome check --write",
    "precompute": "tsx scripts/precompute.ts",
    "test": "vitest",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudokukit/sudokukit.git"
  },
  "keywords": [
    "sudoku",
    "sudokukit",
    "typescript",
    "solver",
    "puzzle"
  ],
  "author": "Coen Mooij",
  "license": "The Unlicense",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/sudokukit/sudokukit/issues"
  },
  "homepage": "https://github.com/sudokukit/sudokukit#readme",
  "dependencies": {
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "~2.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "tsx": "^4.20.4",
    "vitest": "^3.2.4",
    "vitest-tsconfig-paths": "^3.4.1"
  }
}
